{
  "family": "pipeline",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "worker",
      "image": "managedkube/kops:dev",
      "essential": true,
      "entryPoint": [
        "sh",
        "-c"
      ],
      "command": [
        "./containers/kubernetes/clusters/kops/ci-pipeline.sh -i master -u",
        "env.GITHUB_REF"
      ],
      "environment": [
        {
          "name": "KOPS_STATE_STORE",
          "value": "s3://kubernetes-ops-expanse-1234-kops-state-store"
        },
        {
          "name": "ENVIRONMENT_NAME",
          "value": "dev-test"
        },
        {
          "name": "E2E_TESTS_QUICK_MODE",
          "value": "true"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ci-pipeline",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "worker"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::354114410416:role/kubernetesops/fargate/role/execution/kubernetesOpsKopsFargateTaskExecution",
  "taskRoleArn": "arn:aws:iam::354114410416:role/kubernetesops/fargate/role/task/kubernetesOpsKopsFargateTask"
}
