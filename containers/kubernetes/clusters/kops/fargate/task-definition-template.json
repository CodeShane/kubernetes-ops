{
    "family": "pipeline", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "worker", 
            "image": "docker.io/managedkube/kubernetes-pipeline:dev",
            "essential": true, 
            "entryPoint": [
                "/opt/update-cluster-run.sh"
            ], 
            "command": [
                "--initial-branch",
                "master",
                "--updated-to-branch",
                "master"
            ],
            "environment" : [
                { "name" : "KOPS_STATE_STORE", "value": "s3://xxxx"},
                { "name" : "ENVIRONMENT_NAME", "value" : "dev-test" },
                { "name" : "E2E_TESTS_QUICK_MODE", "value": "false"},
                { "name" : "GIT_URL_WITH_DEPLOY_KEY", "value": "https://xxxDEPLOY_KEYxxxx@github.com/ManagedKube/kubernetes-ops.git"}
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ci-pipeline",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "worker"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512",
    "executionRoleArn": "arn:aws:iam:::role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam:::role/kubernetesOpsKopsFargateTask"
}
